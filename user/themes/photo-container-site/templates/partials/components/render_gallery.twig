{% for event in found.result %}
  {%
    include 'partials/components/render_thumb.twig' with {
    'data': event,
    'type': 'home',
    'col': 'col-lg-3 col-md-4 col-sm-6 col-12',
    'show': [
      'icon',
      'photo-name',
      'album-type',
      'album-name',
      'image-count',
      'fav-count'
    ]
  }
  %}
{% endfor %}

<script>
  $( document ).ready(function(){
    $(".search-hits").html({{ found.total }})
  })

  Event.removeEvent(localStorage.endpoint)
</script>