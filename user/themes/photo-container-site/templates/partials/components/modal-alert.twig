<div id="modal-alert" class="modal fade">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div id="modal-alert-header" class="modal-header">
        <h5 class="modal-title title text-uppercase">Alerta</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body text-center">
      MODAL ALERT
      </div>
      <div class="modal-footer">
        <a id="modal-alert-cancel" class="btn btn-secondary" data-dismiss="modal">Cancelar</a>
        <a id="modal-alert-confirm" class="btn">Confirmar</a>
      </div>
    </div>
  </div>
</div>
