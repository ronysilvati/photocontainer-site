{% set slider_images = [url('theme://resources/images/IMG_5093.jpg'),url('theme://resources/images/IMG_5093.jpg')] %}
<div id="home-slider-quotes" class="slider slider-quotes">

  <div class="slider-inner vh cover" role="listbox">
    {% set i = 0 %}
    {% for image in slider_images %}
    <div class="slider-item">
      <img src="{{image}}">
      <div class="slider-caption">
        <i class="icon-quote"></i>
        <blockquote class="blockquote">
          <p>Lorem ipsum dolor sit amet, consectetur adipiscing elit. Integer posuere erat a ante.</p>
          <footer class="blockquote-footer">Lauro Maeda</footer>
        </blockquote>
      </div>
    </div>
    {% set i = i+1 %}
    {% endfor %}
  </div>


</div>
